#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/coffee" ]; then
  "$basedir/coffee"  "$basedir/../html2coffeekup/bin/html2coffeekup" "$@"
  ret=$?
else 
  coffee  "$basedir/../html2coffeekup/bin/html2coffeekup" "$@"
  ret=$?
fi
exit $ret
